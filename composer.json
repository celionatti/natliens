{
  "name": "user/natliens",
  "description": "A Mini Slim PHP Framework",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Natliens\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "authors": [
    {
      "name": "celio natti",
      "email": "celionatti@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "ext-pdo": "*",
    "doctrine/dbal": "^3.6",
    "doctrine/migrations": "^3.6",
    "doctrine/orm": "^2.15",
    "laminas/laminas-diactoros": "^3.0",
    "php-di/php-di": "^7.0",
    "slim/psr7": "^1.6",
    "slim/slim": "^4.11",
    "slim/twig-view": "^3.3",
    "symfony/cache": "^6.2",
    "symfony/console": "^6.2",
    "symfony/twig-bridge": "^6.2",
    "symfony/webpack-encore-bundle": "^1.16",
    "twig/intl-extra": "^3.6",
    "twig/twig": "^3.6",
    "vlucas/phpdotenv": "^5.5",
    "vlucas/valitron": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1"
  }
}
